import threading
import logging

logging.basicConfig(level=logging.DEBUG,
        format='%(threadName)-10s %(message)s'
        )

def worker(numb):
    name =  threading.current_thread().getName
    logging.debug("Im running with input %s" % numb)


    return

threads = []
for i in xrange(4):
    # Construct a thread use Thread class arg(target)
    t = threading.Thread(target=worker, args=(i,)) #note the , after i
    t.start()
