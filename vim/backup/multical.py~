from multiprocessing import Process
import os

def fact(n):
	res = 1
	for i in range(1,1000):
		res *= i
	print 'fac', res

def sumof(n):
	res = 0
	for i in range(10000000):
		res += i
	print 'sum', res

if __name__ == '__main__':
	sumof(1)
	p = Process(target=fact, args=(1,))
	q = Process(target=sumof, args=(1,))
	q.start()
	p.start()
	p.join()
	q.join()
	
	

