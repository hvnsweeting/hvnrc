/**
 * Card Class
 * Identifies Card
 * 
 *	@suit
 *	1	2	3	4
 *	s	c	d	h
 *	@rank
 *	1	2	3	4	...	10	11	12	13	
 *	A	2	3	4	...	10	J	Q	K
 * @subpackage	Libraries
 * @author		ExpressionEngine Dev Team
 */
class CI_Card{

    var $suit = 0;
    var $rank = 0;
    
    public function __construct($rank, $suit)
    {
        $this->rank = $rank;
        $this->suit = $suit; 
    }
    
    public function getCard($rank, $suit)
    { 
    }
    
}
