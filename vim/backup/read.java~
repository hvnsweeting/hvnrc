import java.io.*;

public class read
{
	public static void main (String [] args) throw IOException
	{
		FileReader fr = null;
		int c;
		try
		{
			fr = new FileReader("read.java");
			while((c = fr.read()) != -1)
			{
				System.out.print(c);
			}
		}
		catch(IOException e)
		{
			e.printStackTrace();
		}
		finally {
			if(fr != null)
				fr.close();
	   	}


	}
}
