#include <stdio.h>
#include <stdlib.h>

int compare(void * a, void * b)
{
	return (*(int *) a - *(int * ) b);
}

int main()
{
	int values[] = { 88, 2 ,3, 4, 52, 32, 1,54, 4};
	//number of elements in array
	int nelems = sizeof(values) / sizeof(int);

	//qsort array
	qsort(values, nelems, sizeof(int), compare);

	int n;
	for(n = 0; n < nelems; n++ )
	{
		printf("%d " , values[n]);
	}
	return 0;

}
